{
  "name": "@vercel-cdktf-example/deployment",
  "version": "0.1.0",
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "pnpm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "pnpm i cdktf@next cdktf-cli@next",
    "deploy": "cdktf deploy --auto-approve",
    "destroy": "cdktf destroy --auto-approve"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "cdktf": "^0.15.5",
    "constructs": "^10.1.277",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
